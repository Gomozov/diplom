<h1>Список контролируемых устройств </h1>

<table>
  <tr>
  </tr>

<% @devices.each do |device| %>
  <tr>
    <td><%=h device.device_code %></td>
    <td><%=h device.created_at %></td>
    <td><%= link_to 'Show', device %></td>
    <td><%= link_to 'Destroy', device, :confirm => 'Are you sure?', :method => :delete %></td>
  </tr>
<% end %>
</table>

<br />
